/* Anatomical Locaions */

    DROP TABLE ANATOMICAL_LOC;
    
    CREATE TABLE ANATOMICAL_LOC (
        Code INTEGER PRIMARY KEY AUTOINCREMENT,
        Description VARCHAR(50)
    );

