/* Clinics */
    DROP TABLE CLINIC;
    CREATE TABLE CLINIC (
        Code INT AUTO_INCREMENT,
        Description VARCHAR(50),
        Addr VARCHAR(100),
        City VARCHAR(20),
        StateProv CHAR(2),
        ZIP VARCHAR(10),
        Country CHAR(2),
        Phone VARCHAR(15),
        PRIMARY KEY (Code)
    );
    
