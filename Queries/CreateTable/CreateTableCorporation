/* Corporations */
    DROP TABLE CORPORATION;
    CREATE TABLE CORPORATION (
        Code INT AUTO_INCREMENT,
        Description VARCHAR(50),
        PRIMARY KEY (Code)
    );
    
    INSERT INTO CORPORATION (Description) VALUES
