/* Corporations */
    DROP TABLE CORPORATION;
    CREATE TABLE CORPORATION (
        CorpNo INT AUTO_INCREMENT,
        CorpName VARCHAR(50) UNIQUE,
        PRIMARY KEY (CorpNo)
    );
    
    INSERT INTO CORPORATION (CorpName) VALUES
        ('MediLife Healthcare Corporation'),
        ('HealthLink Medical Group'),
        ('MediCore Hospitals, Inc.'),
        ('MediServe Health Systems'),
        ('WellnessCare Clinics, LLC');