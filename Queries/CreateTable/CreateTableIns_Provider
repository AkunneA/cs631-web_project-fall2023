/* Insurance Provider */
    DROP TABLE INS_PROVIDER;
    CREATE TABLE INS_PROVIDER (
        Code INT AUTO_INCREMENT,
        Description VARCHAR(255),
        PRIMARY KEY (Code)
    );
    
