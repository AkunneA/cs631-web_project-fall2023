/* Rooms */
    DROP TABLE ROOM;
    CREATE TABLE ROOM (
        RoomBed VARCHAR(10),
        Facility INT,
        NurseUnit INT, /* Must be between 1 - 7 */
        Wing VARCHAR(5) CHECK (Wing IN ('Blue', 'Green')),
        PRIMARY KEY (RoomBed),
        FOREIGN KEY (Facility) REFERENCES FACILITY(Code)
    );

