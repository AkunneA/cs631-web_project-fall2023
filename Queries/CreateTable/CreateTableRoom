/* Rooms */
    DROP TABLE ROOM;
    CREATE TABLE ROOM (
        Facility INT,
        NurseUnit INT CHECK (NurseUnit IN (1, 2, 3, 4, 5, 6, 7)),
        Wing VARCHAR(5) CHECK (Wing IN ('Blue', 'Green')),
        RoomBed VARCHAR(4),
        PRIMARY KEY (Facility, RoomBed),
        FOREIGN KEY (Facility) REFERENCES FACILITY(Code)
    );

